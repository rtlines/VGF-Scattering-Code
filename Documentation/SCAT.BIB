@article{Purcell,
abstract = {A method is described for calculation approximate extinction, absorption, and scattering cross-sections for dielectric grains of arbitrary shape, with dimensions comparable to or smaller than the wavelength of the incident radiation. The grain is modeled by an array of N polarizable elements in vacuum. The elements are located on a simple cubic lattice, and N is of order of magnitude 100. The polarizability of an element is such that an unbounded array would exhibit, according to the Clausius-Mossotti relation, the bulk dielectric constant of the grain material. The complex vecto amplitude of each oscillator in the array, which is driven by the field of the incident wave and the fields of all the other oscillators, is determined by an itterative procedure. From the N amplitudes all the cross-sections, including differential cross-sections, are obtained. The method was tested by comparing the cross-sections computed for "spherical" clusters of oscillators with exact Mie theroy values for the corresponding dielectric spheres. Computed cross-sections are presented for five differeent grain shapes and theree different complex refractive indices.},
author = {Edward M. Purcell and Carlton R. Pennypacker},
journal = {Astrophys. J.},
location = {Office},
method = {DDA},
month = {December},
pages = {705-715},
title = {Scattering and Absorption of Light by Nonspherical Dielectric Grains},
volume = {186},
year = {1973},
}
