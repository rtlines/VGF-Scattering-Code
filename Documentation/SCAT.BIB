@article{Draine93,
abstract = {We derive the dispersion relation for electromagnet waves propagation on a lattice of polarizable points. From this dispersion relation we obtain a perscription for choosing dipole polarizabilites so that an infinite latice with finite lattice spacing will mimic a continuum with dielectric constant \eps(\omega). The Discrete Dipole Approximation is used to calculate scatering and absorption by a finite target by re;lacing the target with an array of point dipoles. We compare different prescriptions for determining the dipole polarizabilities. We show that the most accurate results are obtained when the lattice dispersion relation is used to set the plarizabilities.},
author = {B. T. Draine and Jeremy Goodman},
journal = {Astrophys. J.},
location = {Office Home},
method = {DDA},
month = {March},
pages = {685-697},
title = {Beyond {C}lausius-{M}ossotti: Wave Propagation on a Polarizable Point Lattice and the Discrete Dipole Approximation},
volume = {405},
year = {1993},
}
@article{Draine94,
abstract = {The discrete dipole approximation (DDA) for scattering calculations is reviewed, including the relationship between the DDA and other methods. Computational consiterations - use of complex conjugate gradient algorithms and fast fourier transform methods - are discussed. The accuracy of the DDA is tested by using the DDA to compute scattering and absorption by isolated, homogenous spheres, as well as for targets consisting of two contiguous spheres. It is shown that for dielectric naterials (|m|<=2), the DDA permits calculation of scattering and absorption which are accurate to within a few %.},
author = {Bruce T. Draine and Piotr J. Flatau},
journal = {J. Opt. Soc. Am.},
location = {Office},
method = {DDA},
title = {The Discrete-Dipole Approximation for Scattering Calculations},
year = {1994},
}
